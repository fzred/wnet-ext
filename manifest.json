{
  // 清单文件的版本，这个必须写，而且必须是2
  "manifest_version": 2,
  // 插件的名称
  "name": "wnet",
  // 插件的版本
  "version": "1.0.0",
  // 插件描述
  "description": "wnet",
  "browser_action": {
    "default_icon": "img/icon.png",
    // 图标悬停时的标题，可选
    // "default_title": "",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "webRequest",
    "tabs",
    "*://*/*",
    "background"
  ]
}